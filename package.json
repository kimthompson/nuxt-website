{
  "name": "kimthompson.github.io",
  "version": "1.0.0",
  "description": "My portfolio and blog in Nuxt.js",
  "author": "Kim Thompson",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "fix": "cross-env NODE_ENV=development nodemon server/index.js server --fix",
    "build": "nuxt build",
    "prod": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "start": "serve ./dist",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.13",
    "@fortawesome/free-brands-svg-icons": "^5.7.0",
    "@fortawesome/free-solid-svg-icons": "^5.7.0",
    "@nuxtjs/axios": "^5.0.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "node-sass": "^4.10.0",
    "nuxt": "^2.0.0",
    "nuxt-fontawesome": "^0.4.0",
    "sass-loader": "^7.1.0",
    "swing": "^3.1.3",
    "vue-swing": "0.0.10"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "serve": "^10.1.2"
  }
}
